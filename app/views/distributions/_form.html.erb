<%= simple_form_for (@distribution) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :address_1, placeholder: "12 square de Verdun" %>
  <%= f.input :station, placeholder: "Gare de l'Est" %>
  <%= f.button :submit %>
<% end %>


<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyAi-cd_OyaQVKbvoCGRTRSCapQKlqoks9E"></script>
<% end %>
